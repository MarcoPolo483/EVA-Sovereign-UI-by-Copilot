{
  "branches": [
    "main"
  ],
  "preset": "conventionalcommits",
  "plugins": [
    ["@semantic-release/commit-analyzer", {"preset": "conventionalcommits"}],
    ["@semantic-release/release-notes-generator", {"preset": "conventionalcommits"}],
    ["@semantic-release/changelog", {"changelogFile": "CHANGELOG.md"}],
    ["@semantic-release/npm", {"npmPublish": false}],
    ["@semantic-release/git", {"assets": ["CHANGELOG.md", "package.json", "packages/eva-sovereign-ui-react/package.json"], "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"}],
    ["@semantic-release/github", {"failComment": false, "labels": ["release"]}]
  ]
}
{
  "branches": [
    "main",
    {"name": "next", "prerelease": true}
  ],
  "plugins": [
    ["@semantic-release/commit-analyzer", {"preset": "conventionalcommits"}],
    ["@semantic-release/release-notes-generator", {"preset": "conventionalcommits"}],
    ["@semantic-release/changelog", {"changelogFile": "CHANGELOG.md"}],
    ["@semantic-release/npm", {"tarballDir": "dist"}],
    ["@semantic-release/github", {"assets": ["dist/**/*"]}],
    ["@semantic-release/git", {"assets": ["CHANGELOG.md", "package.json"], "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"}]
  ]
}
