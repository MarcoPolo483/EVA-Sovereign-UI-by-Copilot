{
  "name": "@eva-suite/sovereign-ui-react",
  "version": "1.0.0",
  "description": "React wrappers for EVA-Sovereign-UI Web Components",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external @eva-suite/sovereign-ui",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external @eva-suite/sovereign-ui",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eva-suite/sovereign-ui": "^1.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "@eva-suite/sovereign-ui": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.5"
  },
  "keywords": [
    "react",
    "web-components",
    "eva-suite",
    "government",
    "accessibility"
  ],
  "author": "EVA Suite Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eva-suite/EVA-Sovereign-UI-by-Copilot.git",
    "directory": "packages/eva-sovereign-ui-react"
  }
}
